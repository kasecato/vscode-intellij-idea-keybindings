            }, {
                "key": "ctrl+alt+right",
                "mac": "cmd+alt+right",
                "command": "workbench.action.navigateForward",
                "intellij": "Navigate forward"
            },



















            {
                "key": "ctrl+b",
                "mac": "cmd+b",
