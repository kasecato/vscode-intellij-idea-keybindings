            }, {
                "key": "alt+right",
                "mac": "ctrl+right",
                "command": "workbench.action.nextEditor",
                "intellij": "Go to next editor tab"
            },




































            {
                "key": "ctrl+g",
                "mac": "cmd+l",
                "command": "workbench.action.gotoLine",
                "intellij": "Go to line"
            }, {
                "key": "ctrl+e",
                "mac": "cmd+e",
                "command": "workbench.action.openRecent",
